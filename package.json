{
  "name": "website",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": "Vlad Holubiev",
  "main": "pages/index.js",
  "scripts": {
    "analyze": "ANALYZE=1 next build",
    "build": "next build",
    "dev": "next",
    "export": "next export",
    "lint": "eslint . --fix --ext .js,.json,.ts,.tsx --quiet",
    "lint:ci": "eslint . --ext .js,.json,.ts,.tsx --quiet",
    "start": "next start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.{html,md,yml,graphql}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,js,json}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "antd": "3.26.13",
    "react": "16.13.0",
    "react-dom": "16.13.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "7.8.3",
    "@shelf/eslint-config": "0.14.2",
    "@types/node": "12",
    "@zeit/next-css": "1.0.1",
    "babel-plugin-import": "1.13.0",
    "eslint": "6.8.0",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "next": "9.3.0",
    "prettier": "1.19.1",
    "typescript": "3.8.3",
    "webpack-bundle-analyzer": "3.6.1"
  }
}
